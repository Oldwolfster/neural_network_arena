</sql><sql name="RamDB.sql" filename="C:/Users/dad/Documents/RamDB.sql">-- Reference to file &quot;C:/Users/dad/Documents/RamDB.sql&quot; (not supported by this version) --</sql><sql name="SQL 4">SELECT r.*
FROM NNA_history r
JOIN (
    -- Insert the Step 1 query here
    SELECT arena_name, hidden_activation_name, MIN(timestamp) AS best_timestamp
    FROM NNA_history r
    WHERE NOT EXISTS (
        SELECT 1
        FROM NNA_history r2
        WHERE r2.arena_name = r.arena_name
          AND r2.hidden_activation_name = r.hidden_activation_name
          AND (
            r2.accuracy &gt; r.accuracy
            OR (r2.accuracy = r.accuracy AND r2.best_mae &lt; r.best_mae)
            OR (r2.accuracy = r.accuracy AND r2.best_mae = r.best_mae AND r2.final_error &lt; r.final_error)
          )
    )
    GROUP BY arena_name, hidden_activation_name
) winners
ON r.arena_name = winners.arena_name
   AND r.hidden_activation_name = winners.hidden_activation_name
   AND r.timestamp = winners.best_timestamp
order by problem_type, arena_name, accuracy DESC ;
</sql><current_tab id="2"/></tab_sql></sqlb_project>
